	<div class="form-group" class="col-lg-12">
			<input id="tfFiltreCatalogueCategories" type="text"  class="form-control" placeholder="{{ 'mma.productcatalog.searchcataloguecategorie'|trans }}">
		</div>
    <table  class="table table-hover">
    	<thead>
    		<th>ID</th>
    		<th>ParentId</th>
    		<th>EN</th>
    		<th>DE</th>
    		<th>FR</th>
    		<th><a class="fa fa-plus-circle" href="" style="color:#33cc33;font-size:30px; text-align: center;align-content:center; " data-toggle="modal" data-target="#categoryCatalogModal"></a></th>
    		
    	</thead>
    	<tbody id="tbCatalogueCategories">
		    {% for catCat in catalogueCategories %}
        		<tr scope="row">
            		<td scope="col">{{ catCat.id }}</td>
            		<td scope="col">{{ catCat.parentId }} </td>
            		<td scope="col">{{ catCat.en}}</td>
            		<td scope="col">{{ catCat.de }}</td>
            		<td scope="col">{{ catCat.fr }}</td>
        			<td scope="col"><a class="fa fa-edit" href="{{ path('configuration_produit_controller_editer',{'id':catCat.id}) }}"  style="color:darkorange;"></a></td>
        		</tr>
    		{% endfor %}
    	</tbody>
    </table>

	<div id="categoryCatalogModal" id="catalog_category_dialog" class="modal fade" role="dialog">
		<div class="modal-dialog modal-lg">

			<!-- Modal content-->
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal">&times;</button>
					<h4 class="modal-title">Creer une categorie</h4>
				</div>
                    {{ include('configuration/categories/create.html.twig') }}
			</div>
		</div>
	</div>
<script>
    $("#tfFiltreCatalogueCategories").on("keyup", function() {
        var value = $(this).val().toLowerCase();
        $("#tbCatalogueCategories tr").filter(function() {
			$(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
        });
    });
</script>
